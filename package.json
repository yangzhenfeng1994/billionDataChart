{
  "name": "tky",
  "version": "0.1.0",
  "private": true,
  "author": "yangzhenfeng",
  "scripts": {
    "serve": "cross-env vue-cli-service serve",
    "build": "cross-env vue-cli-service build",
    "lint": "cross-env vue-cli-service lint",
    "cz": "cross-env npm run log && git add . && git cz",
    "dev": "cross-env vue-cli-service serve",
    "log": "cross-env conventional-changelog --config ./node_modules/vue-cli-plugin-commitlint/lib/log -i CHANGELOG.md -s -r 0"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "less": "^4.1.3",
    "less-loader": "^6.2.0",
    "vue": "^2.6.11",
    "vue-router": "^3.2.0",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.13",
    "@vue/cli-plugin-eslint": "~4.5.13",
    "@vue/cli-plugin-router": "~4.5.13",
    "@vue/cli-plugin-vuex": "~4.5.13",
    "@vue/cli-service": "~4.5.13",
    "babel-eslint": "^10.1.0",
    "commitizen": "^4.0.3",
    "commitlint": "^8.2.0",
    "conventional-changelog-cli": "^2.0.28",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "husky": "^3.0.9",
    "lint-staged": "^13.0.3",
    "right-pad": "^1.0.1",
    "vue-cli-plugin-commitlint": "~1.0.12",
    "vue-template-compiler": "^2.6.11",
    "cross-env": "^7.0.3"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/vue-cli-plugin-commitlint/lib/cz"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**": [
      "prettier --config .prettierrc --write",
      "eslint --fix",
      "git add"
    ]
  },
  "mail": "yangzhenfeng1994@gmail.com"
}
